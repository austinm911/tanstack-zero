{
  "name": "tanstack-zero-react-router",
  "private": true,
  "type": "module",
  "scripts": {
    "1": "bun test.ts",
    "build": "vite build && tsc",
    "check": "biome check --write",
    "clean": "rm -rf node_modules bun.lock",
    "db:down": "docker compose --env-file .env -f docker/docker-compose.yml down",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:up": "docker compose -f docker/docker-compose.yml up --remove-orphans",
    "dev": "vite",
    "serve": "vite preview",
    "shadcn": "bunx shadcn@canary add",
    "start": "vite --port 3000",
    "test": "vitest run",
    "update-packages": "bunx taze -I --recursive",
    "zero-cache": "source .env && zero-cache-dev -p src/db/schema.zero.ts --log-level debug"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@rocicorp/zero-sqlite3",
    "esbuild"
  ],
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.2",
    "@rocicorp/zero": "^0.16.2025022800",
    "@tailwindcss/vite": "^4.0.12",
    "@tanstack/react-form": "^1.0.5",
    "@tanstack/react-router": "^1.112.18",
    "@tanstack/router-devtools": "^1.114.0",
    "@tanstack/router-plugin": "^1.113.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.40.0",
    "drizzle-zero": "^0.5.1",
    "lucide-react": "^0.479.0",
    "pg": "^8.13.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.12",
    "tailwindcss-animate": "^1.0.7",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@faker-js/faker": "^9.6.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/pg": "^8.11.11",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "drizzle-kit": "^0.30.5",
    "jsdom": "^26.0.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vitest": "^3.0.8",
    "web-vitals": "^4.2.4"
  }
}
